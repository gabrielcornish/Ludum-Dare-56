{script:core_mechanic.0}
on view do
 success.show: "none"
 not_quite.show: "none"
 failure.show: "none"
 risk_btn.show: "none"
 safe_btn.show: "none"
 continue_btn.show: "none"
 
 result: random[6] + 1
 last_result.value: result
 
 if result > 4
  success.show: "solid"
  continue_btn.show: "solid"
 else
  not_quite.show: "solid"
  risk_btn.show: "solid"
  safe_btn.show: "solid"
 end
 
end
{end}

{script:core_mechanic.1}
on click do
 success.show: "none"
 not_quite.show: "none"

 result: random[6] + 1
 last_result.value: last_result.value + result
 
 if last_result.value > 6
  failure.show: "solid"
 elseif last_result.value > 4
  success.show: "solid"
 end
 
 continue_btn.show: "solid"
 risk_btn.show: "none"
 safe_btn.show: "none"
end
{end}

